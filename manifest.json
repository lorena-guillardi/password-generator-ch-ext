{
    "manifest_version": 3,
    "name": "Gerador de Senhas Fortes",
    "version": "1.0",
    "description": "Gera senhas fortes com parâmetros personalizáveis",
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": {
        "16": "src/icons/icon16.png",
        "48": "src/icons/icon48.png",
        "128": "src/icons/icon128.png"
      }
    },
    "permissions": ["storage"]
  }